
managed_by_broker: <%= p("tcp_agent.managed_by_broker") %>
broker_end_point: <%= p("tcp_agent.broker_end_point") %>
broker_username: <%= p("tcp_agent.broker_username") %>
broker_password: <%= p("tcp_agent.broker_password") %>

squid:
  reload_command: "sudo /var/vcap/jobs/haproxy/bin/haproxy_ctl force-reload"
  config_dir: "/var/vcap/jobs/haproxy/config"
  htpasswd_file: "/var/vcap/data/tcp-agent/htpasswd"


static_users:
<% 
  str = p("squid.users").to_yaml 
  str.gsub!(/^---/, '')
%>
<%=str%>
 